{
  "name": "ingress",
  "version": "4.4.0",
  "description": "Composable nodejs applications",
  "main": "lib/main.js",
  "files": [
    "lib",
    "!**/*.spec.js",
    "!**/*.spec.d.ts"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepare": "npm run build",
    "build": "rm -rf lib && npm run lint && tsc",
    "watch:typescript": "tsc -w",
    "watch:test": "jest --watchAll --detectOpenHandles",
    "watch": "npm run build && concurrently -k \"npm:watch:typescript\" \"npm:watch:test\" -c blue,green",
    "test": "jest"
  },
  "repository": {
    "directory": "modules/ingress",
    "type": "git",
    "url": "ssh://git@github.com/ingress/ingress.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      ".*\\.js",
      ".*\\.util\\.spec"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "ignorePatterns": [],
    "overrides": [
      {
        "files": [
          "*.spec.ts"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "rules": {
      "@typescript-eslint/no-use-before-define": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "no-warning-comments": 1,
      "one-var": [
        "error",
        "consecutive"
      ]
    }
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "server",
    "http",
    "application",
    "framework",
    "composable",
    "declarative",
    "middleware"
  ],
  "prettier": {
    "parser": "typescript",
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "dependencies": {
    "@hapi/subtext": "~7.0.3",
    "@ingress/di": "^2.3.0",
    "@ingress/http-status": "^2.3.0",
    "app-builder": "^6.2.2",
    "destroy": "~1.0.4",
    "find-my-way": "^3.0.5",
    "on-finished": "~2.3.0",
    "reflect-annotations": "^3.3.0",
    "rxjs": "~6.6.3",
    "secure-json-parse": "^2.1.0",
    "uuid": "^8.3.2",
    "ws": "^7.4.1"
  },
  "peerDependencies": {
    "@types/ws": "^7.4.0"
  },
  "peerDependenciesMeta": {
    "@types/ws": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/destroy": "~1.0.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/on-finished": "~2.3.1",
    "@types/secure-json-parse": "^1.0.2",
    "@types/sinon": "^9.0.10",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "concurrently": "^5.3.0",
    "cross-fetch": "~3.0.6",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "~7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-watch": "^7.0.0",
    "get-port": "~5.1.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "sinon": "^9.2.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "gitHead": "6296e5f924b953795fd7572963184bc8b6479c71"
}
