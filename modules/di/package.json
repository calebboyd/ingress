{
  "name": "@ingress/di",
  "version": "2.3.0",
  "description": "Server-side dependency injection for ingress",
  "main": "lib/index.js",
  "files": [
    "lib",
    "!**/*.spec.js",
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && npm run lint && tsc",
    "lint": "eslint \"src/**/*\" --fix --ext \"*.ts\"",
    "prepare": "npm run build",
    "test": "jest",
    "watch": "npm run build && concurrently -k \"npm:watch:typescript\" \"npm:watch:test\" -c blue,green",
    "watch:test": "jest --watchAll",
    "watch:typescript": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ingress/ingress.git"
  },
  "keywords": [
    "server di",
    "node dependency injection",
    "dependency",
    "injection"
  ],
  "author": "Caleb Boyd <caleb.boyd@hotmail.com> (https://github.com/calebboyd)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ingress/ingress/issues"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.19",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "~7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "injection-js": "^2.4.0",
    "tslib": "^2.0.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      ".*\\.js"
    ]
  },
  "prettier": {
    "parser": "typescript",
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "ignorePatterns": [
      "*.d.ts"
    ],
    "overrides": [
      {
        "files": [
          "*.spec.ts"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/no-use-before-define": 0
    }
  },
  "gitHead": "6296e5f924b953795fd7572963184bc8b6479c71"
}
